{
  "name": "nestjs-monorepo",
  "description": "nestjs monorepo",
  "keywords": ["heroku", "mozg", "nestjs"],
  "website": "http://mozg.com.br/",
  "repository": "https://github.com/mozgbrasil/acid-workspaces",
  "logo": "http://mozg.com.br/assets/images/logos/logo.svg",
  "success_url": "/",
  "env": {
    "LANG": {
      "description": "https://devcenter.heroku.com/articles/buildpack-api",
